<?xml version="1.0" encoding="utf-8"?>
<!--番茄钟界面背景-->
<shape
    android:shape="oval"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置背景填充色 -->
    <solid android:color="@color/colorWhite" />
    <!-- 设置边框宽度，边框颜色 暂时设置白色-->
    <stroke
        android:width="2dp"
        android:color="@color/colorWhite" />
    <size
        android:width="250dp"
        android:height="250dp"/>
</shape>
